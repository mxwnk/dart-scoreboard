{
  "name": "dart-scoreboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "prod": "export NODE_ENV=production && ts-node server.ts",
    "build": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "prebuild": "prisma generate && ./scripts/update-version.sh",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "db:up": "docker compose up -d",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "docker:start": "node server.js",
    "predocker:start": "node ./node_modules/prisma/build/index.js migrate deploy",
    "version": "./scripts/update-version.sh"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@shadcn/ui": "^0.0.4",
    "@tailwindcss/postcss": "^4.0.14",
    "@types/qrcode": "^1.5.5",
    "@types/ws": "^8.18.1",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "env-cmd": "^10.1.0",
    "lucide-react": "^0.482.0",
    "next": "15.3.0",
    "postcss": "^8.5.3",
    "qrcode": "^1.5.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.14",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@types/node": "^22",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "nodemon": "^3.1.9",
    "prisma": "6.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
